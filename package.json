{
  "name": "adarshaacharya.com.np",
  "private": true,
  "description": "Portfolio website using gatsby.",
  "author": "Aadarsha Acharya",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix -- --cache",
      "npm run format"
    ]
  },
  "dependencies": {
    "@jlengstorf/get-share-image": "^0.7.1",
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "algoliasearch": "^4.4.0",
    "babel-plugin-styled-components": "^1.10.7",
    "disqus-react": "^1.0.9",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "gatsby": "^2.21.21",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-algolia": "^0.11.2",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-mdx": "^1.2.22",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^3.2.18",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.9",
    "gatsby-remark-autolink-headers": "^2.3.11",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-embedder": "^2.1.1",
    "gatsby-remark-images": "^3.3.2",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-json": "^2.4.3",
    "gatsby-transformer-remark": "^2.8.28",
    "gatsby-transformer-sharp": "^2.5.13",
    "jimp": "^0.16.0",
    "node-sass": "^4.14.1",
    "prism-themes": "^1.4.0",
    "prismjs": "^1.20.0",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-system": "^7.0.3",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.7.0",
    "react-switch": "^5.0.1",
    "simple-icons": "^2.14.0",
    "styled-components": "^5.1.1",
    "swr": "^0.2.3",
    "use-sound": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "commitlint": "^9.1.0",
    "eslint": "^7.7.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.4",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adarshaacharya/adarshaacharya.com.np"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/adarshaacharya/adarshaacharya.com.np/blob/master/LICENSE"
    }
  ]
}
